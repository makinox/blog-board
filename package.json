{
  "name": "blog-board",
  "type": "module",
  "version": "1.0.0",
  "scripts": {
    "dev": "astro dev",
    "dev:network": "astro dev --host",
    "fetch:remote": "node ./scripts/fetch-remote.js",
    "generate:previews": "node scripts/generateDefaultPreview.mjs && node scripts/generatePreviewImages.mjs",
    "build": "npm run generate:previews && astro check && astro build",
    "preview": "astro preview",
    "preview:network": "astro preview --host",
    "astro": "astro",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run"
  },
  "dependencies": {
    "@astrojs/check": "0.9.4",
    "@astrojs/mdx": "4.3.1",
    "@astrojs/react": "^4.3.0",
    "@astrojs/rss": "4.0.12",
    "@astrojs/sitemap": "3.4.1",
    "@fontsource/inter": "^5.0.17",
    "@fontsource/lora": "^5.0.16",
    "@resvg/resvg-js": "^2.6.2",
    "@tailwindcss/typography": "^0.5.10",
    "@tailwindcss/vite": "^4.1.8",
    "@types/react-dom": "^19.1.6",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "astro": "5.12.3",
    "clsx": "^2.1.0",
    "eslint": "^8.57.0",
    "eslint-plugin-astro": "^0.32.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-icons": "^5.5.0",
    "sharp": "^0.33.5",
    "tailwind-merge": "^2.2.2",
    "tailwindcss": "^4.1.8",
    "typescript": "^5.4.2",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/react": "^19.1.8",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitest/ui": "^3.2.4",
    "daisyui": "^5.0.43",
    "fs-extra": "^11.3.0",
    "jsdom": "^26.1.0",
    "node-fetch": "^3.3.2",
    "tsx": "^4.20.3",
    "vitest": "^3.2.4"
  },
  "resolutions": {
    "shiki": "3.8.1"
  }
}